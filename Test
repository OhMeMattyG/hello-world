import java.util.Scanner;
public class SumNums {

	public static void main(String[] args) {
		int N = 3;
		int userNum = 0;
		for (int i = 0; i < N; i++){
		Scanner in = new Scanner(System.in);
		System.out.println("Enter Number: ");
		int num = in.nextInt();
		userNum += num;
		}
		System.out.println("The sum of all numbers is " + userNum);
	}

}
